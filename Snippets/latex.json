	"pmatrix": {
		"prefix": "pmatrix",
		"body": [
			"\\begin{pmatrix}",
			"\t$1",
			"\\end{pmatrix}",
			"$0"
		]
	},
	"vmatrix": {
		"prefix": "vmatrix",
		"body": [
			"\\begin{vmatrix}",
			"\t$1",
			"\\end{vmatrix}",
			"$0"
		]
	},
	"longMk": {
		"prefix": "lmk",
		"body": [
			"$$$1$$ $0"
		]
	},
	"Integral": {
		"prefix": "int",
		"body": [
			"\\int\\limits_{${1:a}}^{${2:b}} $0"
		]
	},
