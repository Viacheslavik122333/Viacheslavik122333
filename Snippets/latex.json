{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Align*": {
		"prefix": "al",
		"body": [
			"\\begin{align*}",
			"\t$0",
			"\\end{align*}"
		]
	},
	"bold": {
		"prefix": "bf",
		"body": [
			"\\textbf{${1:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"italic": {
		"prefix": "it",
		"body": [
			"\\textit{${1:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"underbrace": {
		"prefix": "underbrace",
		"body": [
			"\\underbrace{${1:${TM_SELECTED_TEXT}}}_{$2} $0"
		]
	},
	"overbrace": {
		"prefix": "overbrace",
		"body": [
			"\\overbrace{${1:${TM_SELECTED_TEXT}}}^{$2} $0"
		]
	},
	"underset": {
		"prefix": "underset",
		"body": [
			"\\underset{$1}{${2:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"overset": {
		"prefix": "overset",
		"body": [
			"\\overset{$1}{${2:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"thm": {
		"prefix": "th",
		"body": [
			"\\begin{theorem}",
			"\t$1",
			"\\end{theorem} $0"
		]
	},
	"definition": {
		"prefix": "def",
		"body": [
			"\\begin{definition}",
			"\t$1",
			"\\end{definition} $0"
		]
	},
	"subtheorem": {
		"prefix": "subth",
		"body": [
			"\\begin{subtheorem}",
			"\t$1",
			"\\end{subtheorem} $0"
		]
	},
	"consequense": {
		"prefix": "con",
		"body": [
			"\\begin{consequense}",
			"\t$1",
			"\\end{consequense} $0"
		]
	},
	"lemma": {
		"prefix": "lemma",
		"body": [
			"\\begin{lemma}",
			"\t$1",
			"\\end{lemma} $0"
		]
	},
	"remark": {
		"prefix": "remark",
		"body": [
			"\\begin{remark}",
			"\t$1",
			"\\end{remark} $0"
		]
	},
	"proof": {
		"prefix": "proof",
		"body": [
			"\\begin{proof}",
			"\t$1",
			"\\end{proof} $0"
		]
	},
	"WideTilde": {
		"prefix": "wt",
		"body": [
			"\\widetilde{$0}"
		]
	},
	"OverLine": {
		"prefix": "ov",
		"body": [
			"\\overline{$0}"
		]
	},
	"itemize": {
		"prefix": "item",
		"body": [
			"\\begin{itemize}",
			"\t\\item $1",
			"\\end{itemize}",
			"$0"
		]
	},
	"enumerate": {
		"prefix": "enum",
		"body": [
			"\\begin{enumerate}",
			"\t\\item $1",
			"\\end{enumerate}",
			"$0"
		]
	},
	"Environment": {
		"prefix": "begin",
		"body": [
			"\\begin{${1:name}}",
			"\t$0",
			"\\end{${1:name}}"
		]
	},
	"Text": {
		"prefix": "txt",
		"body": [
			"\\text{$0}"
		]
	},
