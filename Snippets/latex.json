	"average": {
		"prefix": "avg",
		"body": [
			"\\langle $1 \\rangle$0"
		]
	},
	"Sum": {
		"prefix": "sum",
		"body": [
			"\\sum_{${1:i=0}}^{${2:\\n}} $0"
		]
	},
