	"cases": {
		"prefix": "cases",
		"body": [
			"\\begin{cases}",
			"\t$1",
			"\\end{cases}",
			"$0"
		]
	},
	"prod": {
		"prefix": "prod",
		"body": [
			"\\prod\\limits_{${1:i=1}}^{${2:n}}$0"
		]
	},
	"mathbb": {
		"prefix": "mb",
		"body": [
			"\\mathbb{$1} $0"
		]
	},
	"mathcal": {
		"prefix": "mc",
		"body": [
			"\\mathcal{$1} $0"
		]
	},
	"Longrightarrow": {
		"prefix": "ra",
		"body": [
			"\\Longrightarrow $0"
		]
	},
	"Longleftarrow": {
		"prefix": "la",
		"body": [
			"\\Longleftarrow $0"
		]
	},
	"Longleftrightarrow": {
		"prefix": "lra",
		"body": [
			"\\Longleftrightarrow $0"
		]
	},
	"longmapsto": {
		"prefix": "lmap",
		"body": [
			"\\longmapsto $0"
		]
	},
	"infinity": {
		"prefix": "inf",
		"body": [
			"\\infty $0"
		]
	}
}
